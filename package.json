{
  "name": "ui-factory",
  "description": "UI Factory is a modern approach to building static websites using React components and CSS-only interactions.",
  "license": "MIT",
  "author": "AlexY-OS",
  "homepage": "https://github.com/alexy-os/ui-factory",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexy-os/layouter.git"
  },
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:test-button": "vite ./headless/test-button --config ./headless/test-button/vite.config.js",
    "dev:semantic-ui": "vite ./headless/semantic-ui",
    "generate:styles": "tailwindcss -i ./src/index.css -o ./public/style.css --minify",
    "generate:semantic-styles": "tailwindcss -i ./src/semantic/index.css -o ./public/style-semantic.css --minify",
    "generate:html": "tsx src/scripts/generate.ts",
    "generate:semantic": "tsx src/semantic/generate.ts",
    "generate:semantic-map": "tsx src/semantic/generate-semantic-map.ts",
    "generate:semantic-html": "tsx src/semantic/generate-semantic-html.ts",
    "generate:semantic-analyze": "bun run ./headless/semantic-ui/cli.ts analyze",
    "generate:semantic-css": "bun run ./headless/semantic-ui/cli.ts generate",
    "generate:semantic-transform": "bun run ./headless/semantic-ui/cli.ts transform",
    "generate:ui": "bun run generate:semantic-analyze && bun run generate:semantic-css && bun run generate:semantic-transform",
    "generate": "bun run generate:styles && bun run generate:html && bun run generate:semantic"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@types/jsdom": "^21.1.7",
    "@vanilla-extract/css": "^1.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsdom": "^26.0.0",
    "lucide-react": "^0.475.0",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.15",
    "styled-system": "^5.1.5",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.1",
    "postcss": "^8.4.35",
    "tailwindcss": "3.4.16",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
}