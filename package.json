{
  "name": "ui-factory",
  "description": "UI Factory is a modern approach to building static websites using React components and CSS-only interactions.",
  "license": "MIT",
  "author": "AlexY-OS",
  "homepage": "https://github.com/alexy-os/ui-factory",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexy-os/ui-factory.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "version": "0.0.5",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "ui-parser:analyze": "bun ./packages/ui-parser/src/cli/bin.ts analyze",
    "ui-parser:generate": "bun ./packages/ui-parser/src/cli/bin.ts generate",
    "ui-parser:transform": "bun ./packages/ui-parser/src/cli/bin.ts transform",
    "ui-parser:all": "bun ./packages/ui-parser/src/cli/bin.ts all",
    "ui-parser:build": "cd packages/ui-parser && tsc && vite build"
  },
  "dependencies": {
    "highlight.js": "^11.11.1",
    "jsdom": "^26.0.0",
    "lucide-react": "^0.475.0",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^0.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@ui-factory/ui-headless": "workspace:*",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.1",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "tailwindcss": "3.4.16",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
}