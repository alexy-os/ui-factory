{
  "name": "ui-factory",
  "description": "UI Factory is a modern approach to building static websites using React components and CSS-only interactions.",
  "license": "MIT",
  "author": "AlexY-OS",
  "homepage": "https://github.com/alexy-os/ui-factory",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexy-os/ui-factory.git"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "version": "0.0.4",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "generate:styles": "tailwindcss -i ./src/index.css -o ./public/style.css --minify",
    "generate:semantic-styles": "tailwindcss -i ./src/semantic/index.css -o ./public/style-semantic.css --minify",
    "generate:object-analyze": "bun run ./generations/object-ui/cli.ts analyze",
    "generate:object-css": "bun run ./generations/object-ui/cli.ts generate",
    "generate:object-transform": "bun run ./generations/object-ui/cli.ts transform",
    "generate:object": "bun run ./generations/object-ui/cli.ts analyze && bun run ./generations/object-ui/cli.ts generate && bun run ./generations/object-ui/cli.ts transform",
    "generate": "bun run generate:styles && generate:object",
    "ui-parser:analyze": "bun ./packages/ui-parser/src/cli/bin.ts analyze",
    "ui-parser:generate": "bun ./packages/ui-parser/src/cli/bin.ts generate",
    "ui-parser:transform": "bun ./packages/ui-parser/src/cli/bin.ts transform",
    "ui-parser:all": "bun ./packages/ui-parser/src/cli/bin.ts all",
    "ui-parser:build": "cd packages/ui-parser && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-slot": "^1.1.2",
    "@types/jsdom": "^21.1.7",
    "@types/semver": "^7.5.8",
    "@vanilla-extract/css": "^1.17.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jsdom": "^26.0.0",
    "lucide-react": "^0.475.0",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.15",
    "styled-system": "^5.1.5",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^0.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.0.1",
    "postcss": "^8.4.35",
    "tailwindcss": "3.4.16",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  }
}